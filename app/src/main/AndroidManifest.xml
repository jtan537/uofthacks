<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fall2018.csc2017.GameCentre">


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="fall2018.csc2017.GameCentre.LaunchCentre">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="fall2018.csc2017.GameCentre.SlidingTiles.SlidingTilesGameActivity"></activity>
        <activity android:name="fall2018.csc2017.GameCentre.UserInterfaceElements.GameHubActivity"></activity>
        <activity android:name="fall2018.csc2017.GameCentre.SlidingTiles.SlidingTilesStartingActivity"></activity>
        <activity android:name="fall2018.csc2017.GameCentre.UserInterfaceElements.LeaderboardActivity"></activity>
        <activity android:name="fall2018.csc2017.GameCentre.MineSweeper.MineSweeperStartingActivity"></activity>
        <activity android:name="fall2018.csc2017.GameCentre.MineSweeper.MineSweeperGameActivity"></activity>
        <activity android:name="fall2018.csc2017.GameCentre.PowersPlus.PowersPlusStartingActivity"></activity>
        <activity android:name="fall2018.csc2017.GameCentre.PowersPlus.PowersPlusGameActivity"></activity>
    </application>

</manifest>